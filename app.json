{
  "expo": {
    "name": "jobApp",
    "slug": "jobApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olalexy1.jobApp"
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/images/adaptive-icon.png",
        "foregroundImage": "./assets/noImg.png",
        "backgroundColor": "#ffffff",
        "androidStatusBar": {
          "backgroundColor": "#000000"
        },
        "softwareKeyboardLayoutMode": "pan"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.olalexy1.jobApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(jobApp) to use your location."
        }
      ],
      [
        "./plugins/withAndroidMainActivityAttributes",
        {
          "android:windowSoftInputMode" : "adjustPan"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8292a840-bd81-4832-886d-9743eee67aad"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8292a840-bd81-4832-886d-9743eee67aad"
    }
  }
}
